{
  "cli": {
    "version": "13.3.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "distribution": "internal",
        "simulator": true,
        "env": {
          "NODE_ENV": "development",
          "EXPO_PUBLIC_API_URL": "http://localhost",
          "EXPO_PUBLIC_API_PORT": "80"
        }
      }
    },
    "beta": {
      "developmentClient": false,
      "env": {
        "EXPO_PUBLIC_API_URL": "http://ec2-18-222-200-13.us-east-2.compute.amazonaws.com",
        "EXPO_PUBLIC_API_PORT": "80",
        "NODE_ENV": "test"
      },
      "android": {
        "buildType": "app-bundle"
      },
      "ios": {
        "distribution": "store",
        "simulator": false,
        "buildConfiguration": "Release"
      }
    },
    "production": {
      "autoIncrement": true,
      "distribution": "store",
      "env": {
        "EXPO_PUBLIC_API_URL": "http://ec2-18-222-200-13.us-east-2.compute.amazonaws.com",
        "EXPO_PUBLIC_API_PORT": "80",
        "NODE_ENV": "production"
      }
    }
  },
  "submit": {
    "beta": {
      "ios": {
        "appleId": "makani.cartwright@gmail.com",
        "ascAppId": "6738511205",
        "appleTeamId": "Q3Z8A24874"
      },
      "android": {
        "track": "internal"
      }
    },
    "production": {
      "ios": {},
      "android": {
        "track": "production"
      }
    }
  }
}
