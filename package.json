{
  "name": "referendum",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios:build": "eas build --profile development --platform ios --local --output ./ios/build.tar.gz --clear-cache && tar -xzf ./ios/build.tar.gz -C ./ios/",
    "ios:boot": "xcrun simctl boot default && open -a Simulator",
    "ios:install": "xcrun simctl install booted ./ios/referendum.app",
    "ios:server": "xcrun simctl launch default referendum.app && npx expo start --ios --dev-client --clear",
    "ios:start": "yarn ios:boot && (yarn ios:install & yarn ios:server)",
    "ios:run": "(yarn ios:build & yarn ios:boot) && yarn ios:install && yarn ios:launch",
    "ios:kill": "killall Simulator && killall com.apple.CoreSimulator.CoreSimulatorService",
    "ios:clean": "rm -rf ./ios/",
    "ios:shutdown": "xcrun simctl shutdown default",
    "ios:uninstall": "xcrun simctl uninstall default referendum.app",
    "ios:rebuild": "yarn ios:uninstall && yarn ios:shutdown && yarn ios:kill && yarn ios:clean && yarn ios:run",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.3",
    "@react-navigation/bottom-tabs": "^6.6.1",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/stack": "^6.4.1",
    "@redux-devtools/core": "^4.0.0",
    "@reduxjs/toolkit": "^2.2.7",
    "date-fns": "^4.1.0",
    "expo": "^51.0.38",
    "expo-dev-client": "~4.0.28",
    "immutable": "^4.3.4",
    "lucide-react": "^0.441.0",
    "react": "18.2.0",
    "react-native": "0.74.5",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1",
    "react-native-vector-icons": "^10.2.0",
    "react-redux": "^9.1.2",
    "react-test-renderer": "18.2.0",
    "redux": "^5.0.1",
    "redux-devtools-expo-dev-plugin": "^0.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "0.75.3",
    "@react-native/eslint-config": "0.75.3",
    "@react-native/metro-config": "0.75.3",
    "@react-native/typescript-config": "0.75.3",
    "@types/react": "~18.2.79",
    "@types/react-native-vector-icons": "^6.4.18",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^8.19.0",
    "eslint-import-resolver-babel-module": "^5.3.2",
    "eslint-plugin-import": "^2.31.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "typescript": "~5.3.3"
  },
  "engines": {
    "node": ">=18"
  }
}
